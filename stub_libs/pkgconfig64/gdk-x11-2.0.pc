prefix=/usr
exec_prefix=${prefix}
libdir=/opt/lsb/lib64
includedir=/opt/lsb/include
target=x11

Name: GDK
Description: GIMP Drawing Kit (${target} target)
Version: 2.10.4
Requires: gdk-pixbuf-2.0 pangoxft pangoft2
Libs: -L${libdir} -lgdk-${target}-2.0 -lX11
Cflags: -I${includedir}/gtk-2.0 -I${libdir}/gtk-2.0/include -DXTHREADS -D_REENTRANT -DXUSE_MTSAFE_API -I/usr/X11R6/include   
